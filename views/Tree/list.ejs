


<%include ../layout/header.ejs %>
<% if(msg){%>
  <h3><%= msg %></h3>
<%}%>

<table class="tab" border="1" cellpadding="8">
  <tr>
    <th>node id </th>
    <th>node name </th>
    <td>node Parent id </td>
    <th>node image</th>

  </tr>

  <% data.docs.forEach(function(nodes){%>


    <tr>
      <td>" <%= nodes._id %>" </td>

      <td> <%= nodes.name %> </td>
      <td> <%= nodes.Parent_id %> </td>
      
      <td> <img width="150px" height="150px" src="/imgs/<%= nodes.picture %>" /> </td>

    </tr>




<%  }) %>

</table>
<div><ul>
    <% for(var i=1; i<=data.pages ; i++){%>
        <li><a href="/Tree/list/<%=i%>"><%=i%></a></li>
        <%}%>
      </ul>
</div>

<%include ../layout/footer.ejs %>
